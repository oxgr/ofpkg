{
  "name": "ofpkg",
  "version": "0.1.0",
  "description": "A CLI tool to package an openFrameworks project along with necessary addons and optionally, the whole openFrameworks library.",
  "main": "src/app.js",
  "bin": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "build": "pkg .",
    "buildRun": "pkg . && ./dist/ofpkg",
    "buildTest": "npm run build && npm run test",
    "test": "cd /Users/rafael/Documents/of_v0.11.2_osx_release/apps/antimodular/remotePulseActionsTest && /Users/rafael/Documents/experiments/ofpkg/dist/ofpkg . && cd -"
  },
  "pkg": {
    "targets": [
      "node16-macos-x64"
    ],
    "assets": [
      "./ofpkg.config.json"
    ],
    "outputPath": "dist/"
  },
  "keywords": [
    "openFrameworks",
    "pkg"
  ],
  "author": "Faadhi Fauzi",
  "license": "ISC",
  "dependencies": {
    "command-line-args": "^5.2.1",
    "fs-extra": "^10.1.0"
  }
}
